{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,gif,svg}",
      "/fonts/*",
      "/assets/*",
      "/api/*"
    ]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".woff2": "font/woff2",
    ".mp4": "video/mp4"
  },
  "globalHeaders": {
    "Cache-Control": "no-cache"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
